{
    "name": "myExten",
    "version": "1.0",
    "description": "Build an Extension!",
    "browser_action":{
    	"default_icon" :"icon.png",
      "default_popup": "popup.html"
    },
    "background": {
      "scripts": ["popup.js"],
      "persistent": false
    },
    "permissions": ["activeTab", "declarativeContent", "storage", "tabs"],
    "manifest_version": 2
  }